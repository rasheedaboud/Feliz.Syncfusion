<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0</TargetFrameworks>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<PackageId>Feliz.Syncfusion</PackageId>
		<Title>Feliz.Syncfusion</Title>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Version>0.1.13</Version>
		<Authors>Rasheed Aboud</Authors>
		<Company>ARMM Services</Company>
		<Description>Felize style bindings for a handfull of Syncfusion React controls. </Description>
		<Summary>Felize style bindings for a handfull of Syncfusion React controls.</Summary>
		<PackageTags>felize fsharp syncfusion fable safe</PackageTags>
		<RepositoryUrl>https://github.com/rasheedaboud/Felize.Syncfusion</RepositoryUrl>
		<PackageProjectUrl>https://github.com/rasheedaboud/Felize.Syncfusion</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageReleaseNotes>
			Added SfButton, SfSplitButton, 
		</PackageReleaseNotes>
		
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="SfButton.fs" />
		<Compile Include="SfSplitButton.fs" />
		<Compile Include="SfDataManager.fs" />
		<Compile Include="SfGrid.fs" />
		<Compile Include="SfInputs.fs" />
		<Compile Include="SfModal.fs" />
		<Compile Include="SfMenuBar.fs" />
	
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Fable.Core" Version="3.6.2" />
		<PackageReference Include="Feliz" Version="1.58.1" />
		<PackageReference Include="FSharp.Core" Version="5.0.2" />
	</ItemGroup>
	<PropertyGroup>
	<NpmDependencies>
		<NpmPackage Name="@syncfusion/ej2-react-navigations" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-data" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-base" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-react-grids" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-react-buttons" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-react-dropdowns" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-react-calendars" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-react-inputs" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-react-charts" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-react-popups" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-react-schedule" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-react-gantt" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-react-notifications" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-react-filemanager" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-react-charts" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-react-treegrid" Version="gt 19.4.53" ResolutionStrategy="Max" />
		<NpmPackage Name="@syncfusion/ej2-react-progressbar" Version="gt 19.4.53" ResolutionStrategy="Max" />
	</NpmDependencies>
</PropertyGroup>
</Project>


